<VirtualHost *:81>
    ServerName xn--var-xla.net
    ServerAlias www.xn--var-xla.net aevar.net www.aevar.net
    DocumentRoot /var/www/xn--var-xla.net

    <FilesMatch "\.(ico|jpg|jpeg|gif|png|css|js)$">
        ExpiresActive on  
        ExpiresDefault "access plus 11 days"
    </FilesMatch>
</VirtualHost>
