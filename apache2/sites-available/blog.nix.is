<VirtualHost *:81>
    ServerName blog.nix.is
    DocumentRoot /var/www/blog.nix.is/
    AddHandler fcgid-script .php
    FCGIWrapper /usr/bin/php5-cgi .php

    <FilesMatch "\.(ico|jpg|jpeg|gif|png|css|js)$">
        ExpiresActive on  
        ExpiresDefault "access plus 11 days"
    </FilesMatch>
</VirtualHost>
